{
  "version": 2,
  "name": "nexa-client",
  "buildCommand": "cd client && npm run build",
  "outputDirectory": "client/build",
  "installCommand": "cd client && npm install",
  "framework": "create-react-app",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "@react_app_api_url"
  }
}
